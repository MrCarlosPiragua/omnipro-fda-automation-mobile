{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "view_branch_search_results",
  "type": "object",
  "required": ["_fahorro", "search"],
  "additionalProperties": true,
  "properties": {
    "_fahorro": {
      "type": "object",
      "required": ["appInteractionEvents", "appInformation"],
      "additionalProperties": true,
      "properties": {
        "appInteractionEvents": {
          "type": "object",
          "required": ["viewBranchSearchResults"],
          "additionalProperties": true,
          "properties": {
            "viewBranchSearchResults": {
              "type": "object",
              "required": ["value"],
              "additionalProperties": true,
              "properties": {
                "value": { "type": "number" }
              }
            }
          }
        },
        "appInformation": {
          "type": "object",
          "required": ["appStateDetails"],
          "additionalProperties": true,
          "properties": {
            "appStateDetails": {
              "type": "object",
              "required": ["screenName"],
              "additionalProperties": true,
              "properties": {
                "screenName": { "type": "string" }
              }
            }
          }
        }
      }
    },
    "search": {
      "type": "object",
      "required": ["keywords", "_fahorro"],
      "additionalProperties": true,
      "properties": {
        "keywords": { "type": "string" },
        "_fahorro": {
          "type": "object",
          "required": ["quantityResults"],
          "additionalProperties": true,
          "properties": {
            "quantityResults": { "type": "integer" }
          }
        }
      }
    }
  }
}
