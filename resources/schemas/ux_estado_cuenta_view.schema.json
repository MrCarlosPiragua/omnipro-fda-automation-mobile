{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "entity - ux_estado_cuenta_view",
  "type": "object",
  "required": ["_fahorro", "commerce"],
  "additionalProperties": true,
  "properties": {
    "_fahorro": {
      "type": "object",
      "required": ["appInteractionEvents", "appInformation", "appInteractionAttributes"],
      "additionalProperties": true,
      "properties": {
        "appInteractionEvents": {
          "type": "object",
          "required": ["estadoCuentaTap"],
          "additionalProperties": true,
          "properties": {
            "estadoCuentaTap": {
              "type": "object",
              "required": ["value"],
              "additionalProperties": true,
              "properties": {
                "value": { "type": "number" }
              }
            }
          }
        },
        "appInformation": {
          "type": "object",
          "required": ["appStateDetails"],
          "additionalProperties": true,
          "properties": {
            "appStateDetails": {
              "type": "object",
              "required": ["screenName"],
              "additionalProperties": true,
              "properties": {
                "screenName": { "type": "string" }
              }
            }
          }
        },
        "appInteractionAttributes": {
          "type": "object",
          "required": ["monederoId"],
          "additionalProperties": true,
          "properties": {
            "monederoId": { "type": "string" }
          }
        }
      }
    },
    "commerce": {
      "type": "object",
      "required": ["order"],
      "additionalProperties": true,
      "properties": {
        "order": {
          "type": "object",
          "required": ["_fahorro"],
          "additionalProperties": true,
          "properties": {
            "_fahorro": {
              "type": "object",
              "required": ["saldoMonedero"],
              "additionalProperties": true,
              "properties": {
                "saldoMonedero": { "type": "number" }
              }
            }
          }
        }
      }
    }
  }
}
