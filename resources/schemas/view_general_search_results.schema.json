{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "entity - view_general_search_results",
  "type": "object",
  "required": ["_fahorro", "search", "productListItems"],
  "additionalProperties": true,
  "properties": {
    "_fahorro": {
      "type": "object",
      "required": ["appInteractionEvents", "appInformation"],
      "additionalProperties": true,
      "properties": {
        "appInteractionEvents": {
          "type": "object",
          "required": ["viewGeneralSearchResults"],
          "additionalProperties": true,
          "properties": {
            "viewGeneralSearchResults": {
              "type": "object",
              "required": ["value"],
              "additionalProperties": true,
              "properties": {
                "value": { "type": "number" }
              }
            }
          }
        },
        "appInformation": {
          "type": "object",
          "required": ["appStateDetails"],
          "additionalProperties": true,
          "properties": {
            "appStateDetails": {
              "type": "object",
              "required": ["screenName"],
              "additionalProperties": true,
              "properties": {
                "screenName": { "type": "string" }
              }
            }
          }
        }
      }
    },

    "search": {
      "type": "object",
      "required": ["keywords", "_fahorro"],
      "additionalProperties": true,
      "properties": {
        "keywords": { "type": "string" },
        "_fahorro": {
          "type": "object",
          "required": ["quantityResults", "searchType"],
          "additionalProperties": true,
          "properties": {
            "quantityResults": { "type": "integer" },
            "searchType": { "type": "string" }
          }
        }
      }
    },

    "productListItems": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["name", "SKU", "priceTotal", "discountAmount", "_fahorro"],
        "additionalProperties": true,
        "properties": {
          "product": { "type": "string" },
          "name": { "type": "string" },
          "SKU": { "type": "string" },
          "priceTotal": { "type": "number" },
          "discountAmount": { "type": "number" },

          "_fahorro": {
            "type": "object",
            "required": [
              "productBrand",
              "position",
              "productPrice",
              "originalPrice",
              "taxAmount",
              "ownBrand",
              "requiresRecipe"
            ],
            "additionalProperties": true,
            "properties": {
              "productFeatures": { "type": ["string", "null"] },
              "productIngredients": { "type": ["string", "null"] },
              "productBrand": { "type": "string" },
              "productAvailable": { "type": "boolean" },
              "subscription": { "type": "boolean" },
              "position": { "type": "string" },
              "taxAmount": { "type": "number" },
              "productPrice": { "type": "number" },
              "originalPrice": { "type": "number" },
              "ownBrand": { "type": "boolean" },
              "requiresRecipe": { "type": "boolean" },
              "productPromotions": {
                "type": "array",
                "items": {
                  "type": "object",
                  "additionalProperties": true,
                  "properties": {
                    "appliedPromotion": { "type": "string" },
                    "promoType": { "type": "string" },
                    "discount": { "type": "number" },
                    "promotionId": { "type": ["string", "number"] }
                  }
                }
              }
            }
          },

          "productCategories": {
            "type": "array",
            "items": {
              "type": "object",
              "required": ["categoryName"],
              "additionalProperties": true,
              "properties": {
                "categoryName": { "type": "string" },
                "categoryPath": { "type": "string" },
                "categoryID": { "type": "string" }
              }
            }
          }
        }
      }
    }
  }
}