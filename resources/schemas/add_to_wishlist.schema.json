{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "entity - add_to_wishlist (productDetail)",
  "type": "object",
  "required": ["_fahorro", "commerce", "productListItems"],
  "additionalProperties": true,
  "properties": {
    "_fahorro": {
      "type": "object",
      "required": ["appInteractionAttributes", "appInteractionEvents", "appInformation"],
      "additionalProperties": true,
      "properties": {
        "appInteractionAttributes": {
          "type": "object",
          "required": ["environment"],
          "additionalProperties": true,
          "properties": {
            "environment": { "type": "string" }
          }
        },
        "appInteractionEvents": { "type": "object", "additionalProperties": true },
        "appInformation": {
          "type": "object",
          "required": ["appStateDetails"],
          "additionalProperties": true,
          "properties": {
            "appStateDetails": {
              "type": "object",
              "required": ["screenName"],
              "additionalProperties": true,
              "properties": {
                "screenName": { "type": "string" }
              }
            }
          }
        }
      }
    },
    "commerce": {
      "type": "object",
      "required": ["saveForLaters", "order"],
      "additionalProperties": true,
      "properties": {
        "saveForLaters": {
          "type": "object",
          "required": ["value"],
          "additionalProperties": true,
          "properties": {
            "value": { "type": "number" }
          }
        },
        "order": {
          "type": "object",
          "required": ["currencyCode"],
          "additionalProperties": true,
          "properties": {
            "currencyCode": { "type": "string" }
          }
        }
      }
    },
    "productListItems": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": [
          "_fahorro",
          "name",
          "SKU",
          "product",
          "priceTotal",
          "discountAmount",
          "productCategories"
        ],
        "additionalProperties": true,
        "properties": {
          "_fahorro": {
            "type": "object",
            "required": [
              "position",
              "productPrice",
              "originalPrice",
              "taxAmount",
              "requiresRecipe",
              "ownBrand",
              "productBrand"
            ],
            "additionalProperties": true,
            "properties": {
              "position": { "type": "string" },
              "productPrice": { "type": "number" },
              "originalPrice": { "type": "number" },
              "taxAmount": { "type": "number" },
              "requiresRecipe": { "type": "boolean" },
              "ownBrand": { "type": "boolean" },
              "productBrand": { "type": "string" },
              "productAvailable": { "type": "boolean" },
              "productPromotions": {
                "type": "array",
                "items": {
                  "type": "object",
                  "additionalProperties": true,
                  "properties": {
                    "discount": { "type": "number" },
                    "promoType": { "type": "string" },
                    "appliedPromotion": { "type": "string" },
                    "promotionId": { "type": "string" }
                  }
                }
              },
              "productIngredients": { "type": "string" },
              "subscription": { "type": "boolean" },
              "productFeatures": { "type": "string" }
            }
          },
          "productCategories": {
            "type": "array",
            "minItems": 1,
            "items": {
              "type": "object",
              "required": ["categoryName", "categoryPath", "categoryID"],
              "additionalProperties": true,
              "properties": {
                "categoryName": { "type": "string" },
                "categoryPath": { "type": "string" },
                "categoryID": { "type": "string" }
              }
            }
          },
          "product": { "type": "string" },
          "quantity": { "type": "number" },
          "priceTotal": { "type": "number" },
          "name": { "type": "string" },
          "discountAmount": { "type": "number" },
          "SKU": { "type": "string" }
        }
      }
    }
  }
}