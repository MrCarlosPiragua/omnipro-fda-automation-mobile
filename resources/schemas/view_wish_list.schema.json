{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "entity - view_wish_list",
  "type": "object",
  "additionalProperties": true,
  "required": ["_fahorro", "eventType", "productListItems"],
  "properties": {
    "eventType": { "type": "string", "const": "application.view_wish_list" },

    "_fahorro": {
      "type": "object",
      "additionalProperties": true,
      "required": ["appInteractionEvents", "appInformation"],
      "properties": {
        "appInteractionEvents": {
          "type": "object",
          "additionalProperties": true,
          "required": ["viewWishList"],
          "properties": {
            "viewWishList": {
              "type": "object",
              "additionalProperties": true,
              "required": ["value"],
              "properties": {
                "value": { "type": "number" }
              }
            }
          }
        },
        "appInformation": {
          "type": "object",
          "additionalProperties": true,
          "required": ["appStateDetails"],
          "properties": {
            "appStateDetails": {
              "type": "object",
              "additionalProperties": true,
              "required": ["screenName"],
              "properties": {
                "screenName": { "type": "string" }
              }
            }
          }
        }
      }
    },

    "commerce": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "order": {
          "type": "object",
          "additionalProperties": true,
          "properties": {
            "currencyCode": { "type": "string" }
          }
        }
      }
    },

    "productListItems": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": true,
        "required": ["_fahorro", "product", "priceTotal", "name", "discountAmount", "SKU"],
        "properties": {
          "_fahorro": {
            "type": "object",
            "additionalProperties": true,
            "required": [
              "productBrand",
              "position",
              "productPrice",
              "originalPrice",
              "taxAmount",
              "ownBrand",
              "requiresRecipe"
            ],
            "properties": {
              "productBrand": { "type": "string" },
              "position": { "type": "string" },
              "productPrice": { "type": "number" },
              "originalPrice": { "type": "number" },
              "taxAmount": { "type": "number" },
              "ownBrand": { "type": "boolean" },
              "requiresRecipe": { "type": "boolean" },
              "productAvailable": { "type": "boolean" },
              "productFeatures": { "type": "string" },
              "productIngredients": { "type": "string" },
              "subscription": { "type": "boolean" },
              "productPromotions": {
                "type": "array",
                "items": {
                  "type": "object",
                  "additionalProperties": true,
                  "required": ["discount", "promoType", "appliedPromotion"],
                  "properties": {
                    "discount": { "type": "number" },
                    "promoType": { "type": "string" },
                    "appliedPromotion": { "type": "string" },
                    "promotionId": { "type": "string" }
                  }
                }
              }
            }
          },

          "productCategories": {
            "type": "array",
            "items": {
              "type": "object",
              "additionalProperties": true,
              "required": ["categoryName", "categoryPath", "categoryID"],
              "properties": {
                "categoryName": { "type": "string" },
                "categoryPath": { "type": "string" },
                "categoryID": { "type": "string" }
              }
            }
          },

          "product": { "type": "string" },
          "priceTotal": { "type": "number" },
          "name": { "type": "string" },
          "discountAmount": { "type": "number" },
          "SKU": { "type": "string" }
        }
      }
    }
  }
}