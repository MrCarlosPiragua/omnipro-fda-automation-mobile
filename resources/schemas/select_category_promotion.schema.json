{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "select_category_promotion",
  "type": "object",
  "required": ["_fahorro"],
  "additionalProperties": true,
  "properties": {
    "_fahorro": {
      "type": "object",
      "required": ["appInteractionEvents", "appInformation", "appInteractionAttributes"],
      "additionalProperties": true,
      "properties": {
        "appInteractionEvents": {
          "type": "object",
          "required": ["selectCategoryPromotion"],
          "additionalProperties": true,
          "properties": {
            "selectCategoryPromotion": {
              "type": "object",
              "required": ["value"],
              "additionalProperties": true,
              "properties": {
                "value": { "type": "number" }
              }
            }
          }
        },
        "appInformation": {
          "type": "object",
          "required": ["appStateDetails"],
          "additionalProperties": true,
          "properties": {
            "appStateDetails": {
              "type": "object",
              "required": ["screenName"],
              "additionalProperties": true,
              "properties": {
                "screenName": { "type": "string" }
              }
            }
          }
        },
        "appInteractionAttributes": {
          "type": "object",
          "required": ["optionSelected"],
          "additionalProperties": true,
          "properties": {
            "optionSelected": { "type": "string" }
          }
        }
      }
    }
  }
}
