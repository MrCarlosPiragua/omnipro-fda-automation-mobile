{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "entity - delete_shopping_cart",
  "type": "object",
  "required": ["_fahorro"],
  "additionalProperties": true,
  "properties": {
    "_fahorro": {
      "type": "object",
      "required": ["appInteractionEvents", "appInformation"],
      "additionalProperties": true,
      "properties": {
        "appInteractionEvents": {
          "type": "object",
          "required": ["deleteShoppingCart"],
          "additionalProperties": true,
          "properties": {
            "deleteShoppingCart": {
              "type": "object",
              "required": ["value"],
              "additionalProperties": true,
              "properties": {
                "value": { "type": "number" }
              }
            }
          }
        },
        "appInformation": {
          "type": "object",
          "required": ["appStateDetails"],
          "additionalProperties": true,
          "properties": {
            "appStateDetails": {
              "type": "object",
              "required": ["screenName"],
              "additionalProperties": true,
              "properties": {
                "screenName": { "type": "string" }
              }
            }
          }
        },
        "appInteractionAttributes": {
          "type": "object",
          "additionalProperties": true,
          "properties": {
            "environment": { "type": "string" }
          }
        }
      }
    },
    "commerce": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "cart": {
          "type": "object",
          "required": ["cartID"],
          "additionalProperties": true,
          "properties": {
            "cartID": { "type": "string" }
          }
        },
        "order": {
          "type": "object",
          "required": ["currencyCode", "priceTotal"],
          "additionalProperties": true,
          "properties": {
            "currencyCode": { "type": "string" },
            "priceTotal": { "type": "number" },
            "_fahorro": {
              "type": "object",
              "required": ["totalProducts"],
              "additionalProperties": true,
              "properties": {
                "totalProducts": { "type": "number" }
              }
            }
          }
        }
      }
    },
    "productListItems": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": true,
        "required": ["name", "SKU", "priceTotal", "quantity", "discountAmount", "_fahorro"],
        "properties": {
          "name": { "type": "string" },
          "SKU": { "type": "string" },
          "priceTotal": { "type": "number" },
          "quantity": { "type": "integer" },
          "discountAmount": { "type": "number" },
          "_fahorro": {
            "type": "object",
            "additionalProperties": true,
            "required": ["productBrand", "position", "productPrice", "originalPrice", "taxAmount", "ownBrand", "requiresRecipe"],
            "properties": {
              "productBrand": { "type": "string" },
              "position": { "type": "string" },
              "productPrice": { "type": "number" },
              "originalPrice": { "type": "number" },
              "taxAmount": { "type": "number" },
              "ownBrand": { "type": "boolean" },
              "requiresRecipe": { "type": "boolean" },
              "productPromotions": {
                "type": "array",
                "items": {
                  "type": "object",
                  "additionalProperties": true,
                  "required": ["appliedPromotion", "discount", "promoType"],
                  "properties": {
                    "appliedPromotion": { "type": "string" },
                    "discount": { "type": "number" },
                    "promoType": { "type": "string" },
                    "promotionId": { "oneOf": [{ "type": "number" }, { "type": "string" }] }
                  }
                }
              }
            }
          },
          "productCategories": {
            "type": "array",
            "items": {
              "type": "object",
              "additionalProperties": true,
              "properties": {
                "categoryName": { "type": "string" }
              }
            }
          }
        }
      }
    }
  }
}
