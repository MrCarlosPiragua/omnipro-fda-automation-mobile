{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "option_tap - screenName requerido",
  "type": "object",
  "required": ["_fahorro"],
  "additionalProperties": true,
  "properties": {
    "_fahorro": {
      "type": "object",
      "required": ["appInformation"],
      "additionalProperties": true,
      "properties": {
        "appInformation": {
          "type": "object",
          "required": ["appStateDetails"],
          "additionalProperties": true,
          "properties": {
            "appStateDetails": {
              "type": "object",
              "required": ["screenName"],
              "additionalProperties": true,
              "properties": {
                "screenName": { "type": "string" }
              }
            }
          }
        }
      }
    }
  }
}
