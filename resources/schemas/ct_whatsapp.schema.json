{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ct_whatsapp",
  "type": "object",
  "required": ["_fahorro", "mobilePhone"],
  "additionalProperties": true,
  "properties": {
    "_fahorro": {
      "type": "object",
      "required": ["appInteractionEvents", "appInformation"],
      "additionalProperties": true,
      "properties": {
        "appInteractionEvents": {
          "type": "object",
          "required": ["ctWhatsapp"],
          "additionalProperties": true,
          "properties": {
            "ctWhatsapp": {
              "type": "object",
              "required": ["value"],
              "additionalProperties": true,
              "properties": {
                "value": { "type": "number" }
              }
            }
          }
        },
        "appInformation": {
          "type": "object",
          "required": ["appStateDetails"],
          "additionalProperties": true,
          "properties": {
            "appStateDetails": {
              "type": "object",
              "required": ["screenName"],
              "additionalProperties": true,
              "properties": {
                "screenName": { "type": "string" }
              }
            }
          }
        }
      }
    },
    "mobilePhone": {
      "type": "object",
      "required": ["number"],
      "additionalProperties": true,
      "properties": {
        "number": { "type": "string" }
      }
    }
  }
}
