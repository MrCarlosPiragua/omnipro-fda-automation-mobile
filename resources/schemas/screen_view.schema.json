{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "entity - screen_view",
  "type": "object",
  "required": ["_fahorro"],
  "additionalProperties": true,
  "properties": {
    "_fahorro": {
      "type": "object",
      "required": ["appInteractionEvents", "appInformation"],
      "additionalProperties": true,
      "properties": {
        "appInteractionEvents": {
          "type": "object",
          "required": ["screenView"],
          "additionalProperties": true,
          "properties": {
            "screenView": {
              "type": "object",
              "required": ["value"],
              "additionalProperties": true,
              "properties": {
                "value": { "type": "number" }
              }
            }
          }
        },
        "appInformation": {
          "type": "object",
          "required": ["appStateDetails"],
          "additionalProperties": true,
          "properties": {
            "appStateDetails": {
              "type": "object",
              "required": [
                "screenName",
                "screenReferring",
                "screenType",
                "statusLogin",
                "screenCategory"
              ],
              "additionalProperties": true,
              "properties": {
                "screenName": { "type": "string" },
                "screenReferring": { "type": "string" },
                "screenType": { "type": "string" },
                "statusLogin": { "type": "boolean" },
                "screenCategory": { "type": "string" }
              }
            }
          }
        }
      }
    }
  }
}
