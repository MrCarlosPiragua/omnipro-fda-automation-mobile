{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Fahorro Event - show_item_list (m√≠nimo requerido)",
  "type": "object",
  "required": ["entity", "commerce"],
  "additionalProperties": true,
  "properties": {
    "entity": {
      "type": "object",
      "required": ["_fahorro"],
      "additionalProperties": true,
      "properties": {
        "_fahorro": {
          "type": "object",
          "required": [
            "appInteractionEvents",
            "appInformation",
            "appInteractionAttributes"
          ],
          "additionalProperties": true,
          "properties": {
            "appInteractionEvents": {
              "type": "object",
              "required": ["showItemList"],
              "additionalProperties": true,
              "properties": {
                "showItemList": {
                  "type": "object",
                  "required": ["value"],
                  "additionalProperties": true,
                  "properties": {
                    "value": { "type": "number" }
                  }
                }
              }
            },
            "appInformation": {
              "type": "object",
              "required": ["appStateDetails"],
              "additionalProperties": true,
              "properties": {
                "appStateDetails": {
                  "type": "object",
                  "required": ["screenName"],
                  "additionalProperties": true,
                  "properties": {
                    "screenName": { "type": "string" }
                  }
                }
              }
            },
            "appInteractionAttributes": {
              "type": "object",
              "required": ["listName", "listId"],
              "additionalProperties": true,
              "properties": {
                "listName": { "type": "string" },
                "listId": { "type": "string" }
              }
            }
          }
        }
      }
    },
    "commerce": {
      "type": "object",
      "required": ["order"],
      "additionalProperties": true,
      "properties": {
        "order": {
          "type": "object",
          "required": ["currencyCode", "_fahorro"],
          "additionalProperties": true,
          "properties": {
            "currencyCode": { "type": "string" },
            "_fahorro": {
              "type": "object",
              "required": ["totalProducts"],
              "additionalProperties": true,
              "properties": {
                "totalProducts": { "type": "number" }
              }
            }
          }
        }
      }
    }
  }
}
