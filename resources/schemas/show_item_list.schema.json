{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "entity - show_item_list (/home_route)",
  "type": "object",
  "required": ["_fahorro"],
  "additionalProperties": true,
  "properties": {
    "_fahorro": {
      "type": "object",
      "required": ["appInteractionAttributes", "appInteractionEvents", "appInformation"],
      "additionalProperties": true,
      "properties": {
        "appInteractionAttributes": {
          "type": "object",
          "required": ["listId", "listName", "environment"],
          "additionalProperties": true,
          "properties": {
            "listId": { "type": "string" },
            "listName": { "type": "string" },
            "environment": { "type": "string" }
          }
        },
        "appInteractionEvents": {
          "type": "object",
          "required": ["showItemList"],
          "additionalProperties": true,
          "properties": {
            "showItemList": {
              "type": "object",
              "required": ["value"],
              "additionalProperties": true,
              "properties": {
                "value": { "type": "number" }
              }
            }
          }
        },
        "appInformation": {
          "type": "object",
          "required": ["appStateDetails"],
          "additionalProperties": true,
          "properties": {
            "appStateDetails": {
              "type": "object",
              "required": ["screenName"],
              "additionalProperties": true,
              "properties": {
                "screenName": { "type": "string" }
              }
            }
          }
        }
      }
    },
    "commerce": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "order": {
          "type": "object",
          "required": ["currencyCode"],
          "additionalProperties": true,
          "properties": {
            "currencyCode": { "type": "string" },
            "_fahorro": {
              "type": "object",
              "additionalProperties": true,
              "properties": {
                "totalProducts": { "type": "integer" }
              }
            }
          }
        }
      }
    }
  }
}
